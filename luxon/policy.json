{
    "role:root": "'Root' in req.credentials.roles",
    "role:ops": "'Operations' in req.credentials.roles",
    "role:admin": "'Administrator' in req.credentials.roles",
    "role:support": "'Support' in req.credentials.roles",
    "role:services": "'Services Administrator' in req.credentials.roles",
    "role:accounts": "'Account Manager' in req.credentials.roles",
    "role:billing": "'Billing' in req.credentials.roles",
    "role:customer": "'Customer' in req.credentials.roles",
    "role:wholesale": "'Wholesale' in req.credentials.roles",
    "role:minion": "'Minion' in req.credentials.roles",
    "role:endpoint": "'Endpoint' in req.credentials.roles",
    "role:virtualization": "'Virtualization' in req.credentials.roles",
    "staff": "$role:root or $role:ops or $role:support or $role:accounts or $role:billing",
    "customer": "$role:customer or $role:wholesale or $staff",
    "internal": "$role:minion or $role:endpoint or $role:root or $role:ops",
    "login": "req.credentials.authenticated is True",
    "services": "$role:virtualization or $internal",
    "admin": "$role:root or $role:ops or $role:admin or $internal",
    "users:admin": "$role:root or $role:ops or $role:admin or $role:wholesale",
    "users:view": "$staff or $role:admin or $customer or $services",
    "roles:admin": "$role:root",
    "roles:view": "$staff",
    "domains:admin": "$role:root",
    "domains:view": "$login",
    "endpoints:admin": "$role:root",
    "endpoints:view": "$login",
    "tenants:admin": "$role:root or $role:admin or $role:ops or $role:wholesale or $role:accounts",
    "tenants:view": "$staff or $role:admin or $customer or $internal",
    "infrastructure:admin": "$role:root or $role:ops or $internal",
    "infrastructure:view": "$staff or $services or $internal",
    "services:admin": "$role:virtualization or $internal or $role:admin or $infrastructure:admin or $role:services",
    "services:view": "$services:admin or $customer or $role:admin or $internal or $infrastructure:view or $role:services",
    "billing:admin": "$role:root or $role:billing or $internal",
    "billing:view": "$staff or $internal",
    "telemetry:admin": "$infrastructure:admin",
    "telemetry:view": "$infrastructure:view",
    "products:admin": "$infrastructure:admin",
    "products:view": "$infrastructure:view",
    "process:admin": "$infrastructure:admin",
    "process:view": "$infrastructure:view",
    "network:admin": "$infrastructure:admin",
    "network:view": "$infrastructure:view",
    "subscriber:admin": "$infrastructure:admin",
    "subscriber:view": "$infrastructure:view",
    "hosting:admin": "$infrastructure:admin",
    "hosting:view": "$infrastructure:view",
    "mobile:admin": "$infrastructure:admin",
    "mobile:view": "$infrastructure:view",
    "cloud:admin": "$infrastructure:admin",
    "cloud:view": "$infrastructure:view",
    "dc:admin": "$infrastructure:admin",
    "dc:view": "$infrastructure:view",
    "dns:admin": "$infrastructure:admin",
    "dns:view": "$infrastructure:view"
}
